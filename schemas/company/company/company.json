{
  "required": [
    "companyId",
    "bankId",
    "password",
    "name",
    "email",
    "status",
    "region",
    "companyType"
  ],
  "type": "object",
  "properties": {
    "companyId": {
      "type": "string",
      "example": "z2o444",
      "description": "The companyId used while registering the company."
    },
    "bankId": {
      "type": "string",
      "example": "sbin0005999",
      "description": "The id of the bank associated with the company."
    },
    "password": {
      "type": "string",
      "format": "password",
      "description": "The password for the company login."
    },
    "name": {
      "type": "string",
      "description": "The name of the comapny"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "The email associate with the company."
    },
    "platform": {
      "type": "string",
      "nullable": true
    },
    "status": {
      "type": "string"
    },
    "region": {
      "type": "string"
    },
    "companyType": {
      "type": "string"
    },
    "businessLine": {
      "type": "string",
      "nullable": true
    },
    "SIF": {
      "type": "string",
      "nullable": true
    },
    "reports": {
      "type": "array",
      "items": {
        "$ref": "../../../z2o-api.json#/components/schemas/Z2O.Company.Report"
      },
      "nullable": true
    },
    "dataConnection": {
      "$ref": "../../../z2o-api.json#/components/schemas/Z2O.Company.DataConnection",
      "nullable": true
    },
    "createdAt": {
      "type": "string",
      "format": "date-time",
      "nullable": true
    }
  }
}
