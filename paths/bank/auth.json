{
  "post": {
    "tags": ["Bank - Auth"],
    "summary": "Get API Key for your bank",
    "description": "After registration the bank can now login into its account to manage its work.",
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "../../z2o-api.json#/components/schemas/Z2O.Bank.BankLogin"
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "Success",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "../../z2o-api.json#/components/schemas/Z2O.Bank.BankAuth"
            }
          }
        }
      },
      "400": {
        "description": "Required Fields are Missing or Bank ID doesn't exist"
      },
      "403": {
        "description": "Password didn't validate"
      },
      "500": {
        "description": "Internal Server Error"
      }
    }
  }
}
